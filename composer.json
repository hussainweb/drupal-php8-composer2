{
    "name": "hussainweb/test-d9p8c2",
    "description": "",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.0@dev",
        "drupal/core": "^9.1@dev",
        "drupal/core-composer-scaffold": "^9.1@dev",
        "drush/drush": "^9.7.1 | ^10.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "axelerant/db-docker": "^1.0",
        "axelerant/drupal-quality-checker": "^1.0",
        "phpunit/phpunit": "^7.0",
        "weitzman/drupal-test-traits": "^1.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "files": [
            "load.environment.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/core": {
                "ReflectionParameter::getClass": "https://www.drupal.org/files/issues/2020-07-04/3156542-15.patch",
                "CSRF Token validator": "https://www.drupal.org/files/issues/2020-07-04/3156880-2.patch",
                "PDO": "https://www.drupal.org/files/issues/2020-09-13/3156881-16.patch",
                "GDImage": "https://www.drupal.org/files/issues/2020-09-23/3156887-9.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "allowed-packages": [
                "axelerant/drupal-quality-checker"
            ]
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
