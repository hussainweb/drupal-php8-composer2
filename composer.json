{
    "name": "hussainweb/test-d9p8c2",
    "description": "",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "composer/installers": "^1.2",
        "consolidation/annotated-command": "4.x-dev",
        "cweagans/composer-patches": "dev-patch-1",
        "drupal/admin_toolbar": "^2.3",
        "drupal/core": "^9.1@dev",
        "drupal/core-composer-scaffold": "^9.1@dev",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redis": "1.x-dev",
        "drupal/token": "1.x-dev",
        "drush/drush": "10.x-dev",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "axelerant/db-docker": "^1.0",
        "axelerant/drupal-quality-checker": "^1.0",
        "phpunit/phpunit": "^7.0",
        "weitzman/drupal-test-traits": "^1.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "autoload": {
        "files": [
            "load.environment.php",
            "web/core/includes/class_aliases.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:hussainweb/composer-patches.git"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "extra": {
        "composer-exit-on-patch-failure": false,
        "patches": {
            "drupal/core": {
                "Base patch": "https://www.drupal.org/files/issues/2020-10-03/3156595-base-90.patch",
                "ReflectionParameter::getClass": "https://www.drupal.org/files/issues/2020-09-30/3156542-27.patch",
                "PDO": "https://www.drupal.org/files/issues/2020-09-13/3156881-16.patch",
                "GDImage": "https://www.drupal.org/files/issues/2020-09-23/3156887-9.patch",
                "call_user_func_array": "https://www.drupal.org/files/issues/2020-09-30/3174022-12.patch"
            },
            "drupal/token": {
                "Install errors": "https://www.drupal.org/files/issues/2020-09-26/3173359-2.patch"
            },
            "drupal/redis": {
                "Required Parameters follow optional parameters": "https://www.drupal.org/files/issues/2020-10-03/3174685-2.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "allowed-packages": [
                "axelerant/drupal-quality-checker"
            ]
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        }
    }
}
